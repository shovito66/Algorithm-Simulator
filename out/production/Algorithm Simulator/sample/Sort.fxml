            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Rectangle fx:id="rectangle1" arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="200.0" layoutX="450.0" layoutY="400.0" stroke="BLACK" strokeType="INSIDE" width="50.0" />
      <Rectangle fx:id="rectangle2" arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="200.0" layoutX="550.0" layoutY="400.0" stroke="BLACK" strokeType="INSIDE" width="50.0" />
